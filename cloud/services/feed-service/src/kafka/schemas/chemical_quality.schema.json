{
  "$id": "https://example.com/schemas/chemical_quality.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ChemicalQuality",
  "type": "object",
  "properties": {
    "id": { "type": "integer" },
    "productionDate": { "type": "string", "format": "date-time" },
    "feedBatchId": { "type": "integer" },
    "nutrientName": { "type": "string" },
    "amount": { "type": "number" },
    "unit": { "type": ["string", "null"] },
    "createdAt": { "type": "string", "format": "date-time" }
  },
  "required": ["id", "productionDate", "feedBatchId", "nutrientName", "amount", "createdAt"]
}