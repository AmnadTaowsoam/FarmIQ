{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://example.com/schemas/external_factors.deleted.schema.json",
  "title": "ExternalFactorsDeleted",
  "type": "object",
  "required": [
    "traceId",
    "timestamp",
    "source",
    "eventType",
    "data"
  ],
  "properties": {
    "traceId": { "type": "string" },
    "timestamp": { "type": "string", "format": "date-time" },
    "source": { "type": "string" },
    "eventType": { "const": "external_factors.deleted" },
    "data": {
      "type": "object",
      "required": ["id"],
      "properties": {
        "id": { "type": "integer" }
      },
      "additionalProperties": false
    }
  }
}